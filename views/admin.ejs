<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <table>
      <tr>
        <th>Path</th>
        <th>Method</th>
        <th>Count</th>
      </tr>
      <% requests.forEach((req) => { %>
        <tr>
          <td><%= req.path %></td> 
          <td><%= req.method %></td>
          <td><%= req.count %></td>
        </tr>
      <% }); %>
    </table>
  </body>
</html>
